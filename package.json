{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=25%",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test dev 3000 cy:run",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "@styled-icons/boxicons-regular": "^10.34.0",
    "@styled-icons/boxicons-solid": "^10.23.0",
    "@styled-icons/fa-brands": "^10.26.0",
    "@styled-icons/material-outlined": "^10.26.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "@types/lodash.xor": "^4.5.6",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.0",
    "joi": "^17.4.2",
    "lodash.xor": "^4.5.0",
    "next": "11.0.1",
    "next-auth": "^3.28.0",
    "next-pwa": "^3.1.5",
    "next-seo": "^5.4.0",
    "nextjs-progressbar": "^0.0.11",
    "polished": "^4.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.0",
    "styled-components": "^5.2.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@jackfranklin/test-data-bot": "1.3.0",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^12.7.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.1",
    "apollo": "^2.33.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.11.1",
    "cross-env": "^7.0.2",
    "cypress": "^10.1.0",
    "cypress-plugin-stripe-elements": "^1.0.1",
    "eslint": "^7.8.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.1",
    "msw": "^0.35.0",
    "node-fetch": "^2.6.1",
    "plop": "^2.7.4",
    "prettier": "^2.1.1",
    "react-is": "^16.13.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.1.3"
  }
}
